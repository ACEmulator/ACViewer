<Window x:Class="ACViewer.View.Finder"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ACViewer.View"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen" 
        ShowInTaskbar="False"
        FocusManager.FocusedElement="{Binding ElementName=DID}"
        Title="Find Data ID" Height="125 " Width="271" ResizeMode="NoResize" Icon="../Icons/Search_16x.png" Style="{DynamicResource CustomWindowStyle}">
    <Grid Margin="0,0,0,3">
        <Label Content="DID:" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.5,1.173" Margin="25,10,0,0"/>
        <TextBox Name="DID" HorizontalAlignment="Left" Height="24" Margin="63,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="156"/>
        <Button Content="OK" HorizontalAlignment="Left" Margin="63,49,0,0" VerticalAlignment="Top" Width="75" Click="OK_Click" IsDefault="True" />
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="144,49,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.884,0.55" IsCancel="True"/>

    </Grid>
</Window>
